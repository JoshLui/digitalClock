var dispSec:Number;var dispMin:Number;var timerDisplay:String;function setSec(localDispSec:Number){   dispSec = localDispSec;}function setMin(localDispMin:Number){   dispMin = localDispMin;}var timerInterval = setInterval(countDown,1000);function countDown(){ if(dispMin == 0){     if(dispSec == 0){        clearInterval(timerInterval);     }  else {        dispSec --;     } }   else {  dispSec--;  if(dispSec == 59){   dispSec --;  }  if(dispSec == 0){     dispSec = 59;     dispMin --;  }   }  timeDisplay.text = prependZero(dispMin)+":"+prependZero(dispSec);}function prependZero(num){ if(num<10){     num ="0"+ num; } return (num);}setSec(6);setMin(2);
